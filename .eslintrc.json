{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "no-param-reassign": "off",
        "import/extensions": "off",
        "no-tabs": "off",
        "indent": "off",
        "no-use-before-define": "off",
        "no-shadow": "off",
        "max-len": "off"
    }
}
